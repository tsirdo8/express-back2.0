{
  "version": 2,
  "builds": [
    {
      "src": "script.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "DB/**",
          "auth/**",
          "route/**",
          "middlewares/**",
          "config/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "script.js"
    }
  ]
}